// index.js
const num1 = 2; // Define num1
const num2 = 31; // Define num2

const multiply = num1 * num2; // Multiply num1 and num2

function getRandomInt() {
  return Math.floor(Math.random() * Number.MAX_SAFE_INTEGER) + 1;
}

const num3 = 10; // Define num3

const mod = num3 % 6; // Calculate the remainder

const numbers = [10, 5, 20, 15]; // Define an array of numbers

const max = Math.max(...numbers); // Find the maximum number in the array

module.exports = {
  multiply,
  random: getRandomInt,
  mod,
  max,
};
